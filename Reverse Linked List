/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) { val = x; }
 * }
 */
class Solution {
    public ListNode reverseList(ListNode head) {
        if (head == null)
			return null;
		if (head.next == null)
			return head;
		ListNode t = head;
		ListNode prev = null, next;
		while (t != null) {
			next = t.next;
			t.next = prev;
			prev = t;
			t = next;
		}
		return prev;
    }
}
