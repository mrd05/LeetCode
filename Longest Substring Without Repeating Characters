class Solution {
    public int lengthOfLongestSubstring(String s) {

		int ans = 0;
		Set<Character> set = new HashSet<Character>();
		char c[] = s.toCharArray();
		for (int i = 0, j = 0; i < c.length && j < c.length;) {
			if (set.contains(c[j])) {
				set.remove(c[i++]);
			} else {
				set.add(c[j++]);
				ans = Math.max(ans, j - i);
			}
		}
		return ans;
	
    }
}
