class Solution {
    public int lengthOfLongestSubstring(String s) {

		int ans = 0;
		Set<Character> set = new HashSet<Character>();
		for (int i = 0, j = 0; i < s.length() && j < s.length();) {
			if (set.contains(s.charAt(j))) {
				set.remove(s.charAt(i++));
			} else {
				set.add(s.charAt(j++));
				ans = Math.max(ans, j - i);
			}
		}
		return ans;
	
    }
}
